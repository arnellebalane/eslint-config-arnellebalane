{
  "name": "eslint-config-arnellebalane",
  "version": "5.0.0",
  "description": "My personal ESLint configuration.",
  "main": "index.js",
  "author": "Arnelle Balane <arnellebalane@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnellebalane/eslint-config-arnellebalane.git"
  },
  "keywords": [
    "eslint-config"
  ],
  "bugs": {
    "url": "https://github.com/arnellebalane/eslint-config-arnellebalane/issues"
  },
  "homepage": "https://github.com/arnellebalane/eslint-config-arnellebalane#readme",
  "devDependencies": {
    "eslint": "5.8.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-unicorn": "6.0.1"
  },
  "peerDependencies": {
    "eslint-plugin-vue": "5.0.0-beta.3"
  },
  "eslintConfig": {
    "extends": [
      "./index.js",
      "./rules/plugins/import.js",
      "./rules/plugins/unicorn.js"
    ],
    "plugins": [
      "import",
      "unicorn"
    ],
    "env": {
      "node": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
